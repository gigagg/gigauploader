  },
  "homepage": "https://github.com/gigagg/gigauploader#readme",
  "devDependencies": {
    "chai": "^4.2.0",
    "copyfiles": "^2.4.0",
    "mocha": "^6.2.2",
    "tslint": "^5.20.1",
    "typescript": "^3.9.7"
  }
